<?xml version="1.0" encoding="utf-8"?>
<repos xmlns:xi="http://www.w3.org/2001/XInclude">

<!-- See www.centos.org for a list of public mirrors -->
<repo id="base">
<name>CentOS-%{version}-%{arch} - Base</name>
<baseurl>http://mirror.centos.org/centos/%{version}/os/%{arch}/</baseurl>
<gpgkey>http://mirror.centos.org/centos/RPM-GPG-KEY-CentOS-%{version}</gpgkey>
</repo>

<repo id="updates">
