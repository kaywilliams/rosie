<?xml version="1.0" encoding="utf-8"?>
<config-rpm id='azure-vm-config'>
<!-- Deployproject.org currently provides an azure repository with signed
     walinuxagent and microsoft-hyper-v packages. Ideally Microsoft will
     provide in the future. -->
<repo id='azure'>
<baseurl>http://www.deployproject.org/repos/azure</baseurl>
</repo>

<requires>WALinuxAgent</requires>
<requires>microsoft-hyper-v</requires>
</config-rpm>
